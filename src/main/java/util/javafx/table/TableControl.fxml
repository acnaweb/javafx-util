<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<fx:root type="VBox" VBox.vgrow="ALWAYS" maxHeight="400"
	maxWidth="1358.0" xmlns="http://javafx.com/javafx"
	xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<VBox maxHeight="1.7976931348623157E308"
			maxWidth="1.7976931348623157E308" minHeight="-Infinity"
			minWidth="-Infinity" styleClass="panel">
			<children>
				<HBox alignment="BOTTOM_RIGHT"
					maxHeight="1.7976931348623157E308"
					maxWidth="1.7976931348623157E308" minHeight="-Infinity"
					minWidth="-Infinity" prefHeight="40.0" spacing="10.0"
					VBox.vgrow="ALWAYS">
					<children>
						<Button fx:id="btnRefresh" contentDisplay="TOP"
							defaultButton="true" mnemonicParsing="false"
							stylesheets="@../css/bootstrapfx.css" text="Atualizar">
							<styleClass>
								<String fx:value="btn" />
								<String fx:value="btn-sm" />
								<String fx:value="btn-info" />
							</styleClass>
						</Button>
					</children>
					<padding>
						<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
					</padding>
				</HBox>
			</children>
		</VBox>
		<VBox VBox.vgrow="ALWAYS">
			<children>
				<TableView fx:id="table" VBox.vgrow="ALWAYS" />
			</children>
		</VBox>
	</children>
</fx:root>
