<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<fx:root maxWidth="1358.0" prefWidth="1358.0"
	stylesheets="@../css/bootstrapfx.css" type="VBox" VBox.vgrow="ALWAYS"
	xmlns="http://javafx.com/javafx"
	xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<VBox maxHeight="1.7976931348623157E308"
			maxWidth="1.7976931348623157E308" minHeight="-Infinity"
			minWidth="-Infinity" styleClass="panel">
			<children>
				<HBox alignment="BOTTOM_RIGHT"
					maxHeight="1.7976931348623157E308"
					maxWidth="1.7976931348623157E308" minHeight="-Infinity"
					minWidth="-Infinity" prefHeight="40.0" spacing="10.0"
					VBox.vgrow="ALWAYS">
					<children>
						<Button fx:id="btnRefresh" contentDisplay="TOP"
							defaultButton="true" mnemonicParsing="false" text="Atualizar">
							<styleClass>
								<String fx:value="btn" />
								<String fx:value="btn-sm" />
								<String fx:value="btn-info" />
							</styleClass>
						</Button>
					</children>
					<padding>
						<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
					</padding>
				</HBox>
			</children>
		</VBox>
		<VBox maxHeight="-Infinity" prefHeight="250.0"
			VBox.vgrow="ALWAYS">
			<children>
				<TableView fx:id="table" VBox.vgrow="ALWAYS" />
			</children>
		</VBox>
		<VBox VBox.vgrow="ALWAYS">
			<children>
				<ToolBar prefHeight="40.0">
					<items>
						<Button fx:id="btnNew" mnemonicParsing="false" text="+">
							<styleClass>
								<String fx:value="btn" />
								<String fx:value="btn-success" />
								<String fx:value="btn-sm" />
							</styleClass>
						</Button>
						<Button fx:id="btnSave" mnemonicParsing="false"
							text="Salvar">
							<styleClass>
								<String fx:value="btn" />
								<String fx:value="btn-primary" />
								<String fx:value="btn-sm" />
							</styleClass>
						</Button>
						<Button fx:id="btnRemove" mnemonicParsing="false"
							text="Excluir">
							<styleClass>
								<String fx:value="btn" />
								<String fx:value="btn-sm" />
								<String fx:value="btn-danger" />
							</styleClass>
						</Button>
						<Button fx:id="btnCancel" cancelButton="true"
							mnemonicParsing="false" text="Cancel">
							<styleClass>
								<String fx:value="btn" />
								<String fx:value="btn-default" />
								<String fx:value="btn-sm" />
							</styleClass>
						</Button>
					</items>
				</ToolBar>
				<ScrollPane fitToWidth="true" minHeight="300.0"
					VBox.vgrow="ALWAYS">
					<content>
						<GridPane fx:id="gridControls" hgap="5.0"
							styleClass="panel" vgap="5.0">
							<columnConstraints>
								<ColumnConstraints halignment="RIGHT"
									hgrow="SOMETIMES" minWidth="10.0" percentWidth="10.0"
									prefWidth="100.0" />
								<ColumnConstraints hgrow="SOMETIMES"
									minWidth="10.0" percentWidth="50.0" prefWidth="100.0" />
								<ColumnConstraints hgrow="SOMETIMES"
									minWidth="10.0" percentWidth="40.0" prefWidth="100.0" />
							</columnConstraints>
						</GridPane>
					</content>
				</ScrollPane>

			</children>
		</VBox>
	</children>
</fx:root>
